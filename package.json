{
  "name": "@weareathlon/frontend-webpack-boilerplate",
  "version": "4.2.7",
  "description": "Starter project template boilerplate based on Webpack with SASS and babel ES6/7 support.",
  "homepage": "https://github.com/WeAreAthlon/frontend-webpack-boilerplate#readme",
  "scripts": {
    "build": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "watch:externalServer": "webpack --mode=development --watch --externalServer",
    "bundle": "npm ci && npm run watch",
    "bundle:externalServer": "npm ci && npm run watch:externalServer",
    "production": "cross-env NODE_ENV=production webpack --mode=production",
    "lint-sass": "sass-lint -v -q --format=compact",
    "lint-js": "eslint --ext .js src/js/",
    "server": "nodemon ./src/js/data/proxy-server.js 3007"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@https://github.com/Mardzy/front-end-boilerplate.git"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "autoprefixer": "^9.8.4",
    "axios": "^0.19.2",
    "bootstrap": "^4.1.1",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.1",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "ajv": "^6.12.2",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fibers": "^4.0.3",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.9",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.7",
    "url-loader": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
